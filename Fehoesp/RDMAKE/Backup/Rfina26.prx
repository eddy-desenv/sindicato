       SE1->E1_SALDO    := 0
       SE1->E1_VLCRUZ   := val( substr(TMP->CAMPO,035,11) )+(VAL(SUBS(TMP->CAMPO,047,2))/100)
       SE1->E1_VALJUR   := 0
