AADD(_aEstr,{"SANGUE"    , "N" , 15 , 0})
AADD(_aEstr,{"CLIMED"    , "N" , 15 , 0})
AADD(_aEstr,{"ACADFIS"   , "N" , 15 , 0})
AADD(_aEstr,{"SAUNAS"    , "N" , 15 , 0})
