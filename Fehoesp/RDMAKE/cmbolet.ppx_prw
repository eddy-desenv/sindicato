Function U_DCHSBC(_cNum)


cFatores := "5432765432"
cNumBanc := _cNum

nValor   := 0
nSoma    := 0

For i := 1 To 10

    nValor := Val(Substr(cNumBanc, i, 1)) * Val(Substr(cFatores, i, 1))

    nSoma  := nSoma + nValor

Next

wxxx := Mod(nSoma,11)
cDigNsNum := 11 - wxxx

If wxxx == 0 .Or.  wxxx == 1
	cDigNsNum := 0
Endif

cDigNsNum := Strzero(cDigNsNum, 1)

Return (cDigNsNum)


Function U_DCBOSTON(_cNum)


cFatores := "98765432"
cNumBanc := _cNum

nValor   := 0
nSoma    := 0

For i := 1 To 8

    nValor := Val(Substr(cNumBanc, i, 1)) * Val(Substr(cFatores, i, 1))

    nSoma  := nSoma + nValor

Next

nSoma := nSoma * 10
wxxx  := Mod(nSoma,11)
cDigNsNum := wxxx

If wxxx == 10
	cDigNsNum := 0
Endif

cDigNsNum := Strzero(cDigNsNum, 1)

Return (cDigNsNum)


Function U_DCBRASIL(_cNum)


cFatores := "78923456789"
cNumBanc := _cNum

nValor   := 0
nSoma    := 0

For i := 1 To 11

    nValor := Val(Substr(cNumBanc, i, 1)) * Val(Substr(cFatores, i, 1))

    nSoma  := nSoma + nValor

Next

wxxx  := Mod(nSoma,11)
cDigNsNum := wxxx

If wxxx == 10
	cDigNsNum := "X"
	Return (cDigNsNum)
Endif

cDigNsNum := Strzero(cDigNsNum, 1)

Return (cDigNsNum)