Static Function Exclui_Calote()
dbSelectArea("SE1")
dbSetOrder(3)
dbseek( xFilial()+"002")
ProcRegua(RecCount(),22,05)
_nExcluidos := 0
Do While !Eof() .and. ALLTRIM(se1->e1_naturez)=="002"
	IncProc(_cTitulo)
	if SE1->E1_VENCREA<(date()-30)
		if empty(SE1->E1_BAIXA) .and. substr(SE1->E1_PREFIXO,3,1)=="A" 
			dbSelectArea("SE1")
			RecLock("SE1",.F.)
			DBDELETE()
			msunlock()
			_nExcluidos := _nExcluidos + 1
		endif
	Endif
	dbSelectArea("SE1")
	dbSkip()
Enddo
_cTitulo   := "Exclusao Titulos Inadimplentes Assistencial"
dbSelectArea("SE1")
dbSetOrder(3)
dbseek( xFilial()+"0301")
ProcRegua(RecCount(),22,05)
//_nExcluidos := 0
Do While !Eof() .and. ALLTRIM(se1->e1_naturez)=="0301"
	IncProc(_cTitulo)
	if SE1->E1_VENCREA<(date()-30)
		if empty(SE1->E1_BAIXA) .and. substr(SE1->E1_PREFIXO,3,1)=="A" 
			dbSelectArea("SE1")
			RecLock("SE1",.F.)
			DBDELETE()
			msunlock()
			_nExcluidos := _nExcluidos + 1
		endif
	Endif
	dbSelectArea("SE1")
	dbSkip()
Enddo
Tone (800,2)
Tone (1500,2)
