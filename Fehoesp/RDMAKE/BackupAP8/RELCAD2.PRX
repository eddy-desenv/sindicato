Static function ImpREL()
_aEstr := {}
AADD(_aEstr,{"Cidade"    , "C" , 25 , 0})
AADD(_aEstr,{"HOSPITAIS" , "N" , 15 , 0})
AADD(_aEstr,{"ODONTO"    , "N" , 15 , 0})
