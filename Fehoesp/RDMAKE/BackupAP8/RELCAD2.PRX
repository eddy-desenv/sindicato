	TEXCEP     += ETIQ->EXCEP
	TRELIGIAO  += ETIQ->RELIGIAO
	TOUTROS    += ETIQ->OUTROS
	ttotal     += totalcid
	LI++
	dbselectarea("etiq")
	dbskip()
