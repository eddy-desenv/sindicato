       SE1->E1_PORTADO := "015"
       SE1->E1_AGEDEP := "0249"
       SE1->E1_CONTA   :="050010-0"
       SE1->E1_SITUACA := "1"
       SE1->E1_OCORREN := "01"
       SE1->E1_ORIGEM  := "RFINA26"
