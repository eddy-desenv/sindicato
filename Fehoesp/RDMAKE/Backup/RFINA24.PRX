      DBSELECTAREA("SA1")
      DBSKIP()
      LOOP
    ENDIF
    If !EMPTY(SA1->A1_INAT)
       dbSelectArea("SA1")
       dbskip()
       loop
    EndIf
    If SA1->A1_FILANT == "X"      && Envio para Todos (Geral)
       dbSelectArea("SA1")
       dbskip()
       loop
    EndIf
    IF VAL(SA1->A1_CATEG1) >= 12000
       dbSelectArea("SA1")
       dbskip()
       loop
    EndIf
    DbSelectArea("SZC")
    DbSetorder(2)
    if ! ( dbseek( xFilial() + SA1->A1_ESCCONT ) )
        DBSELECTAREA("SA1")
        reclock("SA1",.F.)
        SA1->A1_ESCCONT := ""
        MSUNLOCK()
    ENDIF
    if mv_par06==1 .and. !empty(sa1->a1_esccont)
       dbSelectArea("SA1")
       dbskip()
       loop
    EndIf
    if mv_par06==2 .and. empty(sa1->a1_esccont)
       dbSelectArea("SA1")
       dbskip()
       loop
    EndIf

