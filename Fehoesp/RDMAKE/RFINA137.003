	/*/
	
	Static Function ValidChar( cText )
	
	LOCAL nI
	
	FOR nI := 1 to Len(cText)
		if !(substr( cText, nI,1) >= "0" .and. substr( cText, nI,1) <= "Z") .and. ;
			!(substr( cText, nI,1) >= "a" .and. substr( cText, nI,1) <= "z") .and. ;
			! substr( cText, nI,1) == " "
			cText := strtran( cText, substr( cText,nI,1), " ")
		endif
	NEXT nI
	
	RETURN cText
	