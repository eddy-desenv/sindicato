    MALA->CEP_SACADO := _cTexto
    _cTexto := SUBSTR(SA1->A1_CEP,6,3)
    MALA->CEPC_SACADO := _cTexto
    _cTexto := SA1->A1_BAIRRO
    MALA->BAI_SACADO := _cTexto
    _cTexto := SA1->A1_MUN
    MALA->MUN_SACADO := _cTexto
    dbcommit()
    msUnlock()
    _nNumSeq := val( _nseq )
    _nNumSeq := _nNumSeq + 1
    _nseq := strzero( _nNumSeq,6 )
    _nRegistros := _nRegistros + 1
    dbSelectArea("SA1")
    dbskip()
End
