#INCLUDE "RWMAKE.CH"
USER FUNCTION CHECAATU()
USE SX2010 ALIAS X2 NEW SHARED
INDE ON X2_CHAVE TO X2
USE SINDHOSP\SX2SH ALIAS SH NEW SHARED
DBGOTOP()
DO WHILE !EOF() 
	DBSELECTAREA("X2")
	DBSEEK(sh->X2_CHAVE)
	IF FOUND()
		DBSELECTAREA("SH")
		RECLOCK("SH",.f.)	
		dbdelete()
		msunlock()
	endif
	dbselectarea("SH")
	dbskip()
enddo